__version__ = "107"


"feeding rss into your irc channel"


from operbot.object import Wd
from operbot.handler import scan
from operbot.util import wait


from operbot import cmds, irc, rss


scan(cmds)
scan(irc)
scan(rss)


Wd.workdir = "/var/lib/operbot/"


def main():
    irc.init()
    rss.init()
    wait()


main()
