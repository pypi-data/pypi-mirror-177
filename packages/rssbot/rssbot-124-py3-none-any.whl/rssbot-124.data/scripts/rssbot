"feeding rss into your irc channel"


__version__ = "124"



from rssbot.object import Wd, cdir
from rssbot.util import wait


from rssbot import irc, rss


Wd.workdir = "/var/lib/rssbot/"


def main():
    cdir(Wd.workdir)
    irc.init()
    rss.init()
    wait()


main()
